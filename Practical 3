public class ReversedNumber {
    public static void main(String[] args) {
        int r=0;
        int num=346;
        while (number > 0) {
            int digit = number % 10; 
            r= r* 10 + digit; 
            number /= 10; 
        }

        
        System.out.println("Reversed is: " + r);
    }
}
